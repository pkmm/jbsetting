      <register name="4" type="4">
        <text>/**
     * 根据二维码的tokendata获取学生档案的信息
     * @param string $tokenData
     * @return Student|\Illuminate\Database\Eloquent\Builder|\Illuminate\Database\Eloquent\Model|null
     * @throws ErrorInvalidQrCode
     */
    public static function getStudentFromQrCode(string $tokenData)
    {
        if (strpos($tokenData, '_') === -1) {
            throw new ErrorInvalidQrCode('不合法的二维码');
        }
        $data = explode(&quot;_&quot;, $tokenData);
        if (!isset($data[0])) {
            throw new ErrorInvalidQrCode('不合法的二维码');
        }
        $studentId = Dec2ThirtyFourManager::decode($data[0]);

        $key = QRCodeManager::buildString('stu', 'p', $tokenData);

        if (QRCodeManager::isExpired($key)) {
            throw new ErrorInvalidQrCode('二维码已经过期');
        }

        return StudentManager::getStudentById($studentId);
    }</text>
